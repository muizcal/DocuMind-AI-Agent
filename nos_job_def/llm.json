{
  "ops": [
    {
      "id": "Agents",
      "args": {
        "gpu": true,
        "image": "docker.io/ollama/ollama:0.12.0",
        "expose": [
          {
            "port": 11434,
            "health_checks": [
              {
                "path": "/api/tags",
                "type": "http",
                "method": "GET",
                "continuous": false,
                "expected_status": 200
              }
            ]
          }
        ],
        "resources": [
          {
            "type": "Ollama",
            "model": "%%global.variables.MODEL%%"
          }
        ]
      },
      "type": "container/run"
    }
  ],
  "meta": {
    "trigger": "dashboard",
    "system_requirements": {
      "required_vram": 10
    }
  },
  "type": "container",
  "global": {
    "variables": {
      "MODEL": "qwen3:8b"
    }
  },
  "version": "0.1"
}
